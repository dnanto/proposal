<!-- https://beast.community/custom_substitution_models -->
<!-- A general time reversible model for DNA.                                          -->
<!-- This element must have parameters for exactly five of the six rates               -->
<!-- The sixth rate has an implied value of 1.0 and all other rates are relative to it -->
<!-- 
This example parameterizes the rate matrix relative to the A<->G transition      
<gtrModel id="gtr1">
	<frequencies> <frequencyModel idref="freqs"/> </frequencies>
	<rateAC> <parameter id="rateAC" value="1.0"/> </rateAC>
	<rateAT> <parameter id="rateAT" value="1.0"/> </rateAT>
	<rateCG> <parameter id="rateCG" value="1.0"/> </rateCG>
	<rateCT> <parameter id="rateCT" value="1.0"/> </rateCT>
	<rateGT> <parameter id="rateGT" value="1.0"/> </rateGT>
</gtrModel>
                                                                                       -->
<models>
    <model id="JC">
        <!-- The JC69 substitution model (Jukes & Cantor, 1969) -->
        <subModel>
            <gtrModel id="submodel">
            <frequencies>
                <frequencyModel dataType="nucleotide">
                    <frequencies>
                        <parameter id="submodel.freq" value="0.25 0.25 0.25 0.25"/>
                    </frequencies>
                </frequencyModel>
            </frequencies>
            <rateAC>
                <parameter id="submodel.ac" value="1.0"/>
            </rateAC>
            <rateAG>
                <parameter id="submodel.ag" value="1.0"/>
            </rateAG>
            <rateAT>
                <parameter id="submodel.at" value="1.0"/>
            </rateAT>
            <rateCG>
                <parameter id="submodel.cg" value="1.0"/>
            </rateCG>
            <rateGT>
                <parameter id="submodel.gt" value="1.0"/>
            </rateGT>
        </gtrModel>
        </subModel>
        <siteModel id="sitemodel">
            <substitutionModel>
                <gtrModel idref="submodel"/>
            </substitutionModel>
            <relativeRate>
                <parameter id="sitemodel.mu" value="1.0" lower="0.0"/>
            </relativeRate>
        </siteModel>
    </model>
