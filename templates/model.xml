<!-- https://beast.community/custom_substitution_models -->
<models>
    <model id="JC">
        <!-- *** DEFINE PARTITION-SPECIFIC SUBSTITUTION MODELS -->
        <!-- *** SUBSTITUTION MODEL FOR PARTITION Gene1 *** -->
        <!-- The JC69 substitution model (Jukes & Cantor, 1969) -->
        <subModel>
            <gtrModel id="subModel_JC">
            <frequencies>
                <frequencyModel dataType="nucleotide">
                    <frequencies>
                        <parameter id="subModel_JC.frequencies" value="0.25 0.25 0.25 0.25"/>
                    </frequencies>
                </frequencyModel>
            </frequencies>
            <rateAC>
                <parameter id="subModel_JC.ac" value="1.0"/>
            </rateAC>
            <rateAG>
                <parameter id="subModel_JC.ag" value="1.0"/>
            </rateAG>
            <rateAT>
                <parameter id="subModel_JC.at" value="1.0"/>
            </rateAT>
            <rateCG>
                <parameter id="subModel_JC.cg" value="1.0"/>
            </rateCG>
            <rateGT>
                <parameter id="subModel_JC.gt" value="1.0"/>
            </rateGT>
        </gtrModel>
        </subModel>
        <!-- *** DEFINE AMONG-SITE HETEROGENEITY (SITE MODELS) *** -->
        <!-- *** SITE MODEL FOR PARTITION Gene1 *** -->
        <siteModel id="siteModel_JC">
            <substitutionModel>
                <gtrModel idref="subModel_JC"/>
            </substitutionModel>
            <relativeRate>
                <parameter id="siteModel_JC.mu" value="1.0" lower="0.0"/>
            </relativeRate>
            <gammaShape gammaCategories="4">
                <parameter id="siteModel_JC.alpha" value="0.5" lower="0.0"/>
            </gammaShape>
        </siteModel>
        <!-- *** DEFINE OPERATORS *** -->
        <operators id="operators" optimizationSchedule="default">
            <scaleOperator scaleFactor="0.75" weight="0.1">
                <parameter idref="siteModel_JC.alpha"/>
            </scaleOperator>
        </operators>
        <prior id="prior">
            <exponentialPrior mean="0.5" offset="0.0">
                <parameter idref="siteModel_JC.alpha"/>
            </exponentialPrior>
        </prior>
        <log>
            <parameter idref="siteModel_JC.alpha"/>
        </log>
    </model>
